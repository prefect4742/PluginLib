<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="PluginPreferenceTheme">
        <attr name="inlineEditTextPreferenceStyle" format="reference" />
        <attr name="pluginListCategoryStyle" format="reference" />
        <attr name="pluginPreferenceStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="PluginListCategory">
        <attr name="pluginClassName" format="string" />
        <attr name="pluginEntryLayout" format="reference" />
    </declare-styleable>

    <declare-styleable name="PluginEditTextPreference">
        <attr name="android:inputType" />
        <attr name="android:digits" />
    </declare-styleable>

    <declare-styleable name="PluginInlineEditTextPreference">
        <attr name="android:inputType" />
        <attr name="android:singleLine" />
        <attr name="android:digits" />
        <attr name="android:hint" />
    </declare-styleable>

    <declare-styleable name="PluginFilePickerPreference">
        <attr name="requestCode" format="integer" />
        <attr name="mimeType" format="string" />
    </declare-styleable>
</resources>
